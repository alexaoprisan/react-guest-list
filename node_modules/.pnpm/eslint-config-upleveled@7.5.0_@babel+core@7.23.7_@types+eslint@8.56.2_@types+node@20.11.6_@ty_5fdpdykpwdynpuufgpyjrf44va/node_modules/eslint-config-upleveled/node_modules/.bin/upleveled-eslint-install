#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/alexandraoprisan/projects/react-guest-list/node_modules/.pnpm/eslint-config-upleveled@7.5.0_@babel+core@7.23.7_@types+eslint@8.56.2_@types+node@20.11.6_@ty_5fdpdykpwdynpuufgpyjrf44va/node_modules/eslint-config-upleveled/bin/node_modules:/Users/alexandraoprisan/projects/react-guest-list/node_modules/.pnpm/eslint-config-upleveled@7.5.0_@babel+core@7.23.7_@types+eslint@8.56.2_@types+node@20.11.6_@ty_5fdpdykpwdynpuufgpyjrf44va/node_modules/eslint-config-upleveled/node_modules:/Users/alexandraoprisan/projects/react-guest-list/node_modules/.pnpm/eslint-config-upleveled@7.5.0_@babel+core@7.23.7_@types+eslint@8.56.2_@types+node@20.11.6_@ty_5fdpdykpwdynpuufgpyjrf44va/node_modules:/Users/alexandraoprisan/projects/react-guest-list/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/alexandraoprisan/projects/react-guest-list/node_modules/.pnpm/eslint-config-upleveled@7.5.0_@babel+core@7.23.7_@types+eslint@8.56.2_@types+node@20.11.6_@ty_5fdpdykpwdynpuufgpyjrf44va/node_modules/eslint-config-upleveled/bin/node_modules:/Users/alexandraoprisan/projects/react-guest-list/node_modules/.pnpm/eslint-config-upleveled@7.5.0_@babel+core@7.23.7_@types+eslint@8.56.2_@types+node@20.11.6_@ty_5fdpdykpwdynpuufgpyjrf44va/node_modules/eslint-config-upleveled/node_modules:/Users/alexandraoprisan/projects/react-guest-list/node_modules/.pnpm/eslint-config-upleveled@7.5.0_@babel+core@7.23.7_@types+eslint@8.56.2_@types+node@20.11.6_@ty_5fdpdykpwdynpuufgpyjrf44va/node_modules:/Users/alexandraoprisan/projects/react-guest-list/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/install.js" "$@"
else
  exec node  "$basedir/../../bin/install.js" "$@"
fi
